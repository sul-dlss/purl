<% if available? %>
  <% if requested_version? %>
    You are viewing this version
  <% else %>
    <%= link_to 'View', version %>
  <% end %>
  | <%= link_to 'Copy URL', version_purl_url(id: version.druid, version: version.version_id), data: { action: "clipboard#copy"} %>
<% else %>
  <%= state.capitalize %>
<% end %>
